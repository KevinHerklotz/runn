{
  "name": "runn",
  "version": "0.1.0",
  "description": "faster \"npm run\"",
  "author": "Kevin Herklotz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://KevinHerklotz@github.com/KevinHerklotz/runn.git"
  },
  "homepage": "https://github.com/KevinHerklotz/runn#readme",
  "bugs": {
    "url": "https://github.com/KevinHerklotz/runn/issues"
  },
  "keywords": [
    "faster",
    "npm",
    "run",
    "one",
    "process",
    "task"
  ],
  "preferGlobal": true,
  "main": {
    "runn": "dist/runn.js"
  },
  "bin": {
    "runn": "dist/runn.js"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-preset-es2015": "^6.3.13",
    "onchange": "^2.0.0"
  },
  "scripts": {
    "compile": "babel --presets es2015 lib/ --out-dir dist/ && chmod +x dist/runn.js",
    "prepublish": "npm run compile",
    "watch": "onchange 'lib/*.js' -- npm run watch:command",
    "watch:command": "npm run compile && runn"
  }
}
